  "paths": {
    "/user/getUserDailyStatistics": {
      "post": {
        "tags": ["user-controller"],
        "summary": "获取用户每日统计",
        "operationId": "getUserDailyStatistics",
        "parameters": [
          {
            "name": "Content-Type",
            "in": "header",
            "required": true,
            "description": "请求内容类型",
            "schema": {
              "type": "string",
              "example": "application/json"
            }
          },
          {
            "name": "X-API-KEY",
            "in": "header",
            "required": true,
            "description": "平台方分配的 API-KEY",
            "schema": {
              "type": "string",
              "example": "your-api-key"
            }
          }
        ],
        "requestBody": {
          "description": "每日统计请求参数（JSON 格式）",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserDailyStatisticsDto"
              },
              "example": {
                "userId": 1308734,
                "jobDate": "2023-10-25"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "成功响应，返回每日统计结果。",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "integer",
                      "description": "状态码（0：成功，1：失败）"
                    },
                    "message": {
                      "type": "string",
                      "description": "返回的消息"
                    },
                    "data": {
                      "type": "object",
                      "description": "每日统计结果",
                      "properties": {
                        "jobDay": {
                          "type": "string",
                          "format": "date",
                          "description": "数据统计的日期（例如：2023-10-01）"
                        },
                        "totalDepositAmount": {
                          "type": "number",
                          "format": "decimal",
                          "description": "当日用户充值总金额"
                        },
                        "totalDepositCount": {
                          "type": "integer",
                          "description": "当日充值总笔数（例如：100笔）"
                        },
                        "totalDepositUserCount": {
                          "type": "integer",
                          "description": "当日进行充值的用户数（例如：50个用户）"
                        },
                        "totalWithdrawAmount": {
                          "type": "number",
                          "format": "decimal",
                          "description": "当日用户提现总金额"
                        },
                        "totalWithdrawCount": {
                          "type": "integer",
                          "description": "当日提现总笔数（例如：80笔）"
                        },
                        "totalWithdrawUserCount": {
                          "type": "integer",
                          "description": "当日进行提现的用户数（例如：40个用户）"
                        },
                        "pendingCollectionAmount": {
                          "type": "number",
                          "format": "decimal",
                          "description": "系统中待归集的总金额"
                        },
                        "collectionCount": {
                          "type": "integer",
                          "description": "当日完成的归集操作笔数（例如：10笔）"
                        },
                        "pendingCollectionAddressCount": {
                          "type": "integer",
                          "description": "当前待归集的地址数量（例如：5个地址）"
                        },
                        "coldWalletBalance": {
                          "type": "number",
                          "format": "decimal",
                          "description": "冷钱包的余额"
                        },
                        "hotWalletBalance": {
                          "type": "number",
                          "format": "decimal",
                          "description": "热钱包的余额"
                        },
                        "additionalTransferHotWallet": {
                          "type": "number",
                          "format": "decimal",
                          "description": "当日热钱包额外转入的金额"
                        }
                      }
                    }
                  }
                },
                "example": {
                  "code": 0,
                  "message": "",
                  "data": {
                    "jobDay": "2023-10-01",
                    "totalDepositAmount": 100000.50,
                    "totalDepositCount": 100,
                    "totalDepositUserCount": 50,
                    "totalWithdrawAmount": 80000.75,
                    "totalWithdrawCount": 80,
                    "totalWithdrawUserCount": 40,
                    "pendingCollectionAmount": 5000.25,
                    "collectionCount": 10,
                    "pendingCollectionAddressCount": 5,
                    "coldWalletBalance": 200000.00,
                    "hotWalletBalance": 150000.00,
                    "additionalTransferHotWallet": 1000.00
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/UnauthorizedError"
          },
          "403": {
            "$ref": "#/components/responses/ForbiddenError"
          },
          "404": {
            "$ref": "#/components/responses/NotFoundError"
          }
        }
      }
    },
    "/api/order/speedUp": {
      "post": {
        "tags": ["order-controller"],
        "summary": "提交加速存币单请求",
        "operationId": "speedUp",
        "parameters": [
          {
            "name": "Content-Type",
            "in": "header",
            "required": true,
            "description": "请求内容类型",
            "schema": {
              "type": "string",
              "example": "application/json"
            }
          },
          {
            "name": "X-API-KEY",
            "in": "header",
            "required": true,
            "description": "平台方分配的 API-KEY",
            "schema": {
              "type": "string",
              "example": "your-api-key"
            }
          }
        ],
        "requestBody": {
          "description": "加速存币单请求参数（JSON 格式）。",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/WithdrawRequestDto"
              },
              "example": {
                "callBackId": "D782243635",
                "userId": 1,
                "tenantUserId": "tenant123",
                "tenantType": 1,
                "amount": 0.5,
                "symbol": "TRX",
                "chainName": "TRON",
                "txId": "tx12345",
                "fromAddr": "fromAddress123",
                "toAddr": "toAddress123",
                "tag": ""
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "成功返回加速存币单结果。",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WithdrawResponse"
                },
                "example": {
                  "code": 0,
                  "data": {
                    "id": "order12345",
                    "tenantUserId": "tenant123"
                  },
                  "message": ""
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/UnauthorizedError"
          },
          "403": {
            "$ref": "#/components/responses/ForbiddenError"
          },
          "404": {
            "$ref": "#/components/responses/NotFoundError"
          }
        }
      }
    },
