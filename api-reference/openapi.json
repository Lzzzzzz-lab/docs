            "type": "string",
            "description": "链名称"
          },
          "symbol": {
            "type": "string",
            "description": "货币符号"
          },
          "tenantUserId": {
            "type": "string",
            "description": "商户用户标识"
          },
          "env": {
            "type": "string",
            "description": "环境标识"
          },
          "userId": {
            "type": "integer",
